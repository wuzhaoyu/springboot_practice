<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.com.wzy.dao.FilmMapper">

    <update id="stockOperate">
        update film set stock = stock + 1 where id = #{id}
